{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/automation-witness",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "nodemon",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/cjs/index.js",
    "clean": "rimraf tsconfig.tsbuildinfo && rimraf ./build && rimraf ./.serverless && rimraf ./.webpack",
    "compile": "yarn gitlint-fix && rimraf ./dist && yarn compile:esm && yarn compile:cjs",
    "compile:cjs": "tsc -p tsconfig.build.cjs.json",
    "compile:esm": "tsc -p tsconfig.build.esm.json",
    "reinstall": "rimraf ./yarn-error.log && rimraf ./node_modules && yarn install && yarn outdated",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\"",
    "test": "jest ./src",
    "test:hash": "jest ./src/hash",
    "test:domain": "jest ./src/domain",
    "test:schema": "jest ./src/schema"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@ethersproject/address": "^5.6.1",
    "@ethersproject/contracts": "^5.6.2",
    "@ethersproject/networks": "^5.6.4",
    "@ethersproject/providers": "^5.6.8",
    "@ethersproject/solidity": "^5.6.1",
    "@xylabs/sdk-api-express-ecs": "^1.2.2",
    "@xylabs/sdk-js": "^2.5.9",
    "@xyo-network/sdk-xyo-client-js": "^2.20.44",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "rollbar": "^2.25.0",
    "source-map-support": "^0.5.21",
    "swagger-autogen": "^2.21.3",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "^28.1.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@xylabs/eslint-config": "^2.3.22",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "babel-jest": "^28.1.1",
    "jest": "^28.1.1",
    "jest-sorted": "^1.0.14",
    "license-checker": "^25.0.1",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.1.1"
}
