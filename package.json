{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/automation-witness-runner",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Automation Witness",
  "sideeffects": false,
  "engines": {
    "node": ">=18 <19"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "nodemon",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/cjs/index.js",
    "test": "jest ./src",
    "test:hash": "jest ./src/hash",
    "test:domain": "jest ./src/domain",
    "test:schema": "jest ./src/schema"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.266.1",
    "@xylabs/assert": "^2.7.1",
    "@xylabs/sdk-api-express-ecs": "^1.3.0",
    "@xyo-network/automation-witness-server": "^2.43.28",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "ethers": "5.7.2",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.1",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@xylabs/eslint-config": "^2.13.11",
    "@xylabs/ts-scripts-yarn3": "^2.13.11",
    "@xylabs/tsconfig": "^2.13.11",
    "babel-jest": "^29.4.2",
    "eslint": "^8.33.0",
    "jest": "^29.4.2",
    "jest-extended": "^3.2.3",
    "jest-mock-extended": "^3.0.1",
    "jest-sorted": "^1.0.14",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "packageManager": "yarn@3.3.1",
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.19"
  }
}
