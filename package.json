{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/automation-witness-runner",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Automation Witness",
  "sideeffects": false,
  "engines": {
    "node": ">=18 <19"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "nodemon",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/cjs/index.js",
    "test": "jest ./src",
    "test:hash": "jest ./src/hash",
    "test:domain": "jest ./src/domain",
    "test:schema": "jest ./src/schema"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.378.0",
    "@xylabs/assert": "^2.9.3",
    "@xylabs/sdk-api-express-ecs": "^1.3.0",
    "@xyo-network/automation-witness-server": "^2.68.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "ethers": "5.7.2",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.2",
    "winston": "^3.10.0",
    "winston-transport": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@xylabs/eslint-config": "^2.19.0",
    "@xylabs/ts-scripts-yarn3": "^2.19.0",
    "@xylabs/tsconfig": "^2.19.0",
    "babel-jest": "^29.6.2",
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "jest-extended": "^4.0.0",
    "jest-mock-extended": "^3.0.4",
    "jest-sorted": "^1.0.14",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@3.3.1",
  "volta": {
    "node": "18.13.0",
    "yarn": "1.22.19"
  }
}
