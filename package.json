{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/automation-witness-runner",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Automation Witness",
  "sideeffects": false,
  "exports": {
    ".": {
      "node": {
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        },
        "require": {
          "types": "./dist/node/index.d.cts",
          "default": "./dist/node/index.js"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.js",
  "module": "dist/node/index.mjs",
  "types": "dist/node/index.d.ts",
  "scripts": {
    "start": "nodemon",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/node/index.js",
    "test": "jest ./src",
    "test:hash": "jest ./src/hash",
    "test:domain": "jest ./src/domain",
    "test:schema": "jest ./src/schema"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.583.0",
    "@xylabs/assert": "^3.5.1",
    "@xylabs/sdk-api-express-ecs": "^2.0.4",
    "@xyo-network/automation-witness-server": "^2.88.6",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "ethers": "6.12.1",
    "express": "^4.19.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.3.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.4",
    "winston": "^3.13.0",
    "winston-transport": "^4.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@xylabs/eslint-config": "^3.10.4",
    "@xylabs/ts-scripts-yarn3": "^3.10.4",
    "@xylabs/tsconfig": "^3.10.4",
    "@xylabs/tsconfig-jest": "^3.10.4",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-mock-extended": "^3.0.7",
    "jest-sorted": "^1.0.15",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@4.1.1",
  "private": true,
  "engines": {
    "node": ">=20.10.0 <21"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "4.1.1"
  },
  "type": "commonjs"
}
